{
  "name": "searchable-input",
  "version": "0.0.0",
  "main": "dist/index.js",
  "repository": "git@github.com:khirayama/searchable-input.git",
  "author": "khirayama",
  "license": "MIT",
  "engines": {
    "node": ">=6.8 <7",
    "npm": ">=3.10 <4"
  },
  "scripts": {
    "pre-build": "mkdir -p ./dist",
    "build:scripts": "npm run pre-build && $(npm bin)/browserify -t coffeeify --extension='.coffee' ./src/scripts/index.coffee > ./dist/searchable-input.js",
    "test": "$(npm bin)/mocha --recursive --compilers coffee:coffee-script/register ./test",
    "watch:scripts": "npm run pre-build && $(npm bin)/watchify ./src/scripts/index.coffee -v -t coffeeify -o ./dist/searchable-input.js"
  },
  "dependencies": {
    "browser-sync": "^2.18.5",
    "browserify": "^13.1.1",
    "coffee-script": "^1.12.2",
    "coffeeify": "^2.1.0",
    "gulp": "^3.9.1",
    "gulp-coffee": "^2.3.3",
    "gulp-if": "^2.0.2",
    "gulp-pleeease": "^2.0.2",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^3.0.0",
    "jsdom": "^9.9.1",
    "lodash": "^4.17.3",
    "mocha": "^3.2.0",
    "power-assert": "^1.4.2",
    "sinon": "^1.17.6",
    "tv4": "^1.2.7",
    "watchify": "^3.8.0"
  }
}
